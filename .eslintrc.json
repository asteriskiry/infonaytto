{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "globals": {
        "React": true
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 2, {
            "SwitchCase": 1
        }],
        "no-console": "off",
        "strict": ["error", "global"],
        "semi": ["error", "never"],
        "comma-dangle": ["error", "only-multiline"],
        "curly": "warn",
        "react/prop-types": [0]
    }
}
